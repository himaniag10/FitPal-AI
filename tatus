[32m+[m[32mconst mongoose = require('mongoose');[m[41m[m
[32m+[m[32mconst bcrypt = require('bcrypt');[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// User Schema Definition[m[41m[m
[32m+[m[32mconst userSchema = new mongoose.Schema({[m[41m[m
[32m+[m[32m    username: {[m[41m[m
[32m+[m[32m        type: String,[m[41m[m
[32m+[m[32m        required: true,[m[41m[m
[32m+[m[32m        unique: true,[m[41m[m
[32m+[m[32m        trim: true, // Remove unnecessary spaces[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    password: {[m[41m[m
[32m+[m[32m        type: String,[m[41m[m
[32m+[m[32m        required: true,[m[41m[m
[32m+[m[32m        validate: {[m[41m[m
[32m+[m[32m            validator: function (v) {[m[41m[m
[32m+[m[32m                // Password must be at least 8 characters long and include a letter and a number[m[41m[m
[32m+[m[32m                return /^(?=.*[A-Za-z])(?=.*\d)[A-Za-z\d@$!%*?&]{8,}$/.test(v);[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            message: 'Password must be at least 8 characters long and include a letter and a number.',[m[41m[m
[32m+[m[32m        },[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    email: {[m[41m[m
[32m+[m[32m        type: String,[m[41m[m
[32m+[m[32m        required: true,[m[41m[m
[32m+[m[32m        unique: true,[m[41m[m
[32m+[m[32m        validate: {[m[41m[m
[32m+[m[32m            validator: function (v) {[m[41m[m
[32m+[m[32m                // Simple email regex[m[41m[m
[32m+[m[32m                return /^[\w-\.]+@([\w-]+\.)+[\w-]{2,4}$/.test(v);[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            message: props => `${props.value} is not a valid email!`,[m[41m[m
[32m+[m[32m        },[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    profilePicture: {[m[41m[m
[32m+[m[32m        type: String,[m[41m[m
[32m+[m[32m        default: 'https://example.com/default-profile-picture.png', // URL for the user's default profile picture[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    age: {[m[41m[m
[32m+[m[32m        type: Number,[m[41m[m
[32m+[m[32m        min: [0, 'Age must be a positive number.'],[m[41m[m
[32m+[m[32m        max: [120, 'Age must be less than 120 years.'],[m[41m[m
[32m+[m[32m        default: null,[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    height: {[m[41m[m
[32m+[m[32m        type: String,[m[41m[m
[32m+[m[32m        default: '',[m[41m[m
[32m+[m[32m        validate: {[m[41m[m
[32m+[m[32m            validator: function (v) {[m[41m[m
[32m+[m[32m                // Simple regex for height format like "5'4"[m[41m[m
[32m+[m[32m                return /^(\d+)'(\d+)"?$/.test(v);[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m            message: props => `${props.value} is not a valid height format!`,[m[41m[m
[32m+[m[32m        },[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    weight: {[m[41m[m
[32m+[m[32m        type: Number,[m[41m[m
[32m+[m[32m        min: [0, 'Weight must be a positive number.'],[m[41m[m
[32m+[m[32m        default: null, // Optional field[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    goals: {[m[41m[m
[32m+[m[32m        type: [String],[m[41m[m
[32m+[m[32m        default: [], // Array of goals[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    fitnessLevel: {[m[41m[m
[32m+[m[32m        type: String,[m[41m[m
[32m+[m[32m        enum: ['Beginner', 'Intermediate', 'Advanced'],[m[41m[m
[32m+[m[32m        default: 'Beginner', // Set a default level[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    dietaryPreferences: {[m[41m[m
[32m+[m[32m        type: String,[m[41m[m
[32m+[m[32m        enum: ['Vegetarian', 'Non-Vegetarian', 'Vegan', 'Pescatarian'],[m[41m[m
[32m+[m[32m        default: 'Non-Vegetarian', // Default dietary preference[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m    activityLog: {[m[41m[m
[32m+[m[32m        type: [[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                date: {[m[41m[m
[32m+[m[32m                    type: Date,[m[41m[m
[32m+[m[32m                    default: Date.now,[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                activityType: {[m[41m[m
[32m+[m[32m                    type: String,[m[41m[m
[32m+[m[32m                    required: [true, 'Activity type is required.'],[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                duration: {[m[41m[m
[32m+[m[32m                    type: Number,[m[41m[m
[32m+[m[32m                    required: [true, 'Duration is required.'],[m[41m[m
[32m+[m[32m                    min: [1, 'Duration must be at least 1 minute.'],[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m                caloriesBurned: {[m[41m[m
[32m+[m[32m                    type: Number,[m[41m[m
[32m+[m[32m                    required: [true, 'Calories burned is required.'],[m[41m[m
[32m+[m[32m                    min: [0, 'Calories burned cannot be negative.'],[m[41m[m
[32m+[m[32m                },[m[41m[m
[32m+[m[32m            },[m[41m[m
[32m+[m[32m        ],[m[41m[m
[32m+[m[32m        default: [],[m[41m[m
[32m+[m[32m    },[m[41m[m
[32m+[m[32m}, { timestamps: true }); // Automatically add createdAt and updatedAt fields[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Hash password before saving the user[m[41m[m
[32m+[m[32muserSchema.pre('save', async function (next) {[m[41m[m
[32m+[m[32m    if (this.isModified('password')) {[m[41m[m
[32m+[m[32m        const salt = await bcrypt.genSalt(12); // Increased salt rounds for better security[m[41m[m
[32m+[m[32m        this.password = await bcrypt.hash(this.password, salt);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    next();[m[41m[m
[32m+[m[32m});[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Instance method to get the user's activity logs[m[41m[m
[32m+[m[32muserSchema.methods.getActivityLogs = function () {[m[41m[m
[32m+[m[32m    return this.activityLog;[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Static method to find user by email[m[41m[m
[32m+[m[32muserSchema.statics.findByEmail = function (email) {[m[41m[m
[32m+[m[32m    return this.findOne({ email });[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Add indexes for performance (frequent query fields)[m[41m[m
[32m+[m[32muserSchema.index({ email: 1 });[m[41m[m
[32m+[m[32muserSchema.index({ username: 1 });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Create and export the User model[m[41m[m
[32m+[m[32mconst User = mongoose.model('User', userSchema);[m[41m[m
[32m+[m[32mmodule.exports = User;[m[41m[m
