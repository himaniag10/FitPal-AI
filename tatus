[1m+++ b/server/middleware/authMiddleware.js[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32m// middleware/authMiddleware.js[m[41m[m
[32m+[m[32mconst jwt = require('jsonwebtoken');[m[41m[m
[32m+[m[41m[m
[32m+[m[32mconst authenticateToken = (req, res, next) => {[m[41m[m
[32m+[m[32m    const token = req.headers['authorization']?.split(' ')[1]; // Get the token from the Authorization header[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if (!token) return res.sendStatus(401); // If no token, return 401 Unauthorized[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    jwt.verify(token, process.env.JWT_SECRET, (err, user) => {[m[41m[m
[32m+[m[32m        if (err) return res.sendStatus(403); // If token is invalid, return 403 Forbidden[m[41m[m
[32m+[m[32m        req.user = user; // Store the user info in the request object[m[41m[m
[32m+[m[32m        next(); // Proceed to the next middleware or route handler[m[41m[m
[32m+[m[32m    });[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32mmodule.exports = authenticateToken;[m[41m[m
