[32m+[m[32m// config/db.js[m[41m[m
[32m+[m[32mconst mongoose = require('mongoose');[m[41m[m
[32m+[m[32mrequire('dotenv').config();[m[41m[m
[32m+[m[41m[m
[32m+[m[32mconst connectDB = async () => {[m[41m[m
[32m+[m[32m  try {[m[41m[m
[32m+[m[32m    await mongoose.connect(process.env.MONGO_URI);[m[41m[m
[32m+[m[32m    console.log('MongoDB connected successfully');[m[41m[m
[32m+[m[32m  } catch (error) {[m[41m[m
[32m+[m[32m    console.error('MongoDB connection error:', error.message);[m[41m[m
[32m+[m[32m    process.exit(1); // Exit process if the connection fails[m[41m[m
[32m+[m[32m  }[m[41m[m
[32m+[m[32m};[m[41m[m
[32m+[m[41m[m
[32m+[m[32m// Connection events to log the status[m[41m[m
[32m+[m[32mmongoose.connection.on('connected', () => {[m[41m[m
[32m+[m[32m  console.log('Mongoose connected to MongoDB');[m[41m[m
[32m+[m[32m});[m[41m[m
[32m+[m[41m[m
[32m+[m[32mmongoose.connection.on('error', (err) => {[m[41m[m
[32m+[m[32m  console.log('Mongoose connection error:', err);[m[41m[m
[32m+[m[32m});[m[41m[m
[32m+[m[41m[m
[32m+[m[32mmongoose.connection.on('disconnected', () => {[m[41m[m
[32m+[m[32m  console.log('Mongoose disconnected from MongoDB');[m[41m[m
[32m+[m[32m});[m[41m[m
[32m+[m[41m[m
[32m+[m[32mmodule.exports = connectDB;[m[41m[m
[1mdiff --git a/server/middleware/authMiddleware.js b/server/middleware/authMiddleware.js[m
